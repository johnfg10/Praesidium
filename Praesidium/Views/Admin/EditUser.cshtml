@model Praesidium.ViewModels.Admin.EditUserViewModel

<div class="edituser">
    <div class="col-sm-6">
        <div class="panel panel-default">
            <div class="panel-heading">
                <h3 class="panel-title">Local Login</h3>
            </div>
            <div class="panel-body">
                <form asp-route="Login">
                    <fieldset>
                        <div class="form-group">
                            <label asp-for="User.UserName">
                                <input class="form-control" value="@Model.User.UserName" asp-for="User.UserName" autofocus>
                            </label>
                        </div>
                        <div class="form-group">
                            <label asp-for="User.Email">
                                <input class="form-control" value="@Model.User.Email" asp-for="User.Email" autofocus>
                            </label>
                        </div>       
                        <div class="form-group">
                            <label asp-for="User.EmailConfirmed">
                                <input type="checkbox" class="form-control" value="@Model.User.EmailConfirmed" asp-for="User.EmailConfirmed" autofocus>
                            </label>
                        </div>
                        <div class="form-group">
                            <label asp-for="User.LockoutEnd">
                                <input type="datetime" class="form-control" value="@Model.User.LockoutEnd" asp-for="User.LockoutEnd" autofocus>
                            </label>
                        </div>
                        <div class="form-group">
                            <label asp-for="User.LockoutEnabled">
                                <input type="checkbox" class="form-control" value="@Model.User.LockoutEnabled" asp-for="User.LockoutEnabled" autofocus>
                            </label>
                        </div>
                        
                        <div class="form-group">
                            <label asp-for="User.PhoneNumber">
                                <input type="phone" class="form-control" value="@Model.User.PhoneNumber" asp-for="User.PhoneNumber" autofocus>
                            </label>
                        </div>
                        
                        <div class="form-group">
                            <label asp-for="User.PhoneNumberConfirmed">
                                <input type="checkbox" class="form-control" value="@Model.User.PhoneNumberConfirmed" asp-for="User.PhoneNumberConfirmed" autofocus>
                            </label>
                        </div>
                        
                        <div class="form-group">
                            <label asp-for="User.AccessFailedCount">
                                <input type="number" class="form-control" value="@Model.User.AccessFailedCount" asp-for="User.AccessFailedCount" autofocus>
                            </label>
                        </div>
                        
                        <div class="form-group">
                            <button class="btn btn-primary" name="button" value="login">Submit</button>
                            <button class="btn btn-default" name="button" value="cancel">Cancel</button>
                        </div>
                    </fieldset>
                </form>
            </div>
        </div>
    </div>
</div>

